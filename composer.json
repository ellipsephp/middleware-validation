{
    "name": "ellipse/middleware-validation",
    "type": "library",
    "description": "The validation middleware of the ellipse framework",
    "keywords": [
        "Psr-15",
        "middleware",
        "validation",
        "validator"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/middleware-validation",
    "support": {
        "issues": "https://github.com/ellipsephp/middleware-validation/issues",
        "source": "https://github.com/ellipsephp/middleware-validation"
    },
    "authors": [
        {
            "name": "Pierre Mallinjoud",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "psr/http-message": "^1.0",
        "http-interop/http-middleware": "^0.4.0",
        "ellipse/validation": "^1.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0-alpha"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Validation\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
